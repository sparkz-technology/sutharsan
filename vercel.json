{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/about", "dest": "/about" },
    { "src": "/work", "dest": "/work" },
    { "src": "/contact", "dest": "/contact" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "env": {
    "DEBUG": "true"
  }
}
