{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/about", "dest": "/dist/about/index.html" },
    { "src": "/work", "dest": "/dist/work/index.html" },
    { "src": "/contact", "dest": "/dist/contact/index.html" },
    { "src": "/(.*)", "dest": "/dist/index.html" }
  ],
  "env": {
    "DEBUG": "true"
  }
}
