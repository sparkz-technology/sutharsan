{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/about", "dest": "/about.html" },
    { "src": "/work", "dest": "/work.html" },
    { "src": "/contact", "dest": "/contact.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "DEBUG": "true"
  }
}
